    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "formik": "^2.4.2",
    "magic-sdk": "^18.4.1",
    "next": "13.4.8",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "decrypt": "^0.0.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.2.3",
    "next": "13.4.8",
    "postcss": "8.4.24",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{css,scss}": [
      "npm run lint:css",
      "npm run format"
    ],
    "*.{json,md}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
