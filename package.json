{
  "name": "gymgenius",
  "version": "1.0.0",
  "description": "Gym Genius README file.",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.44.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint \"**/*.css\" --fix",
      "prettier --write"
    ],
    "*.{json,md,html}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
